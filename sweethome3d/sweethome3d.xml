<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-sh3d">
    <comment>SweetHome3D file</comment>
    <comment xml:lang="pt_BR">Arquivo SweetHome3D</comment>
    <sub-class-of type="application/zip"/>
    <generic-icon name="application-x-sweethome3d"/>
    <icon name="application-x-sweethome3d"/>
    <glob pattern="*.sh3d"/>
  </mime-type>
  <mime-type type="application/x-sh3f">
    <comment>SweetHome3D 3D model library</comment>
    <comment xml:lang="pt_BR">Biblioteca de modelo 3D do SweetHome3D</comment>
    <sub-class-of type="application/zip"/>
    <generic-icon name="application-x-sweethome3d"/>
    <icon name="application-x-sweethome3d"/>
    <glob pattern="*.sh3f"/>
  </mime-type>
  <mime-type type="application/x-sh3t">
    <comment>SweetHome3D texture library</comment>
    <comment xml:lang="pt_BR">Biblioteca de textura do SweetHome3D</comment>
    <sub-class-of type="application/zip"/>
    <generic-icon name="application-x-sweethome3d"/>
    <icon name="application-x-sweethome3d"/>
    <glob pattern="*.sh3t"/>
  </mime-type>
</mime-info>
